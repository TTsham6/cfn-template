{
  "agent": {
    "metrics_collection_interval": 60,
    "region": "ap-northeast-1"
  },
  "logs": {
    "logs_collected": {
        "files": {
            "collect_list": [
                {
                    "file_path": "/var/log/messages",
                    "log_group_name": "amazonlinux-message.log",
                    "timezone": "Local"
                }
            ]
        }
    }
  }
}